package TP3;

public class Obstacles extends Object_Quete{
	//attributs

	//contructeur
	public Obstacles(String nom, int pv,String initial) {
		this.nom = nom;
		this.pv = pv;
		this.initial = initial;
	}
	
	
	public int getObstacles() {
		return pv;
	}
	
	//affichage
	@Override
	public String toString() {
		return nom + " initial " + initial + " enl√®ve " + pv + " pv";
	}

	
	
	
}

